{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "none",
  "exclude": ["**/*.test.ts", "**/__mocks__/**", "**/index.ts"],
  "hideGenerator": true,
  "excludeExternals": true,
  "excludePrivate": true,
  "categorizeByGroup": false,
  "cleanOutputDir": true
}
